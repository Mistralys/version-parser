{
	"name" : "mistralys/version-parser",
	"description" : "Class used to parse version numbers.",
	"type" : "library",
	"license" : "MIT",
	"authors" : 
	[
		{
			"email" : "s.mordziol@mistralys.eu",
			"name" : "Sebastian Mordziol",
			"role" : "Lead"
		}
	],
	"autoload" : 
	{
		"classmap" :
		[
			"src/"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/classes"
		]
	},
	"require": {
		"php": ">=7.4",
		"symfony/polyfill-php80": ">=v1.26.0"
	},
	"require-dev" : 
	{
		"phpunit/phpunit" : ">=8.5",
		"phpstan/phpstan" : ">=0.12"
	},
	"minimum-stability" : "dev",
	"prefer-stable" : true
}
